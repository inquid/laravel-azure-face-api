{
    "name": "inquid/laravel-azure-face-api",
    "description": "Laravel package to access Azure Cognitive Face APIs and do Facial Recognition",
    "type": "library",
    "keywords": [
        "face",
        "laravel",
        "azure"
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "guzzlehttp/guzzle": "~6.0|~7.0",
        "nesbot/carbon": "^2.23"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Niraj Shah",
            "email": "niraj@webniraj.com"
        },
        {
            "name": "Inquid",
            "email": "contact@inquid.co"
        }
    ],
    "autoload": {
        "psr-4": {
            "AzureFace\\": "src/AzureFace/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AzureFace\\AzureFaceServiceProvider"
            ]
        }
    }
}
